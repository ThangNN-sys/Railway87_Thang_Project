package com.vti.frontend;

public class demo1 {

	public static void main(String[] args) {

		int points[] = {1,2,3,4,5,6};
		for (int point : points) {
			System.out.println(point);
		}
		
		for (int i = 0; i < points.length; i++) { 
			int point1 = points[i];
			System.out.println(point1);
		} 


	}

}
